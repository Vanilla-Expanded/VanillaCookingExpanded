<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<PipeSystem.ProcessDef>
		<defName>VCE_MilkIntoCheese</defName>
		<label>Milk into cheese</label>
		<description>Turns 50 milk into 25 cheese.</description>
		<labelOverride>Turn 50 milk into 25 cheese.</labelOverride>
		<priorityInBillList>0</priorityInBillList>
		<spawnOnInteractionCell>true</spawnOnInteractionCell>
		<useIngredients>true</useIngredients>
		<ingredients>
			<li>
				<thing>Milk</thing>
				<countNeeded>50</countNeeded>
			</li>
		</ingredients>
		<ticksQuality>
			<li>600000</li>
			<li>900000</li>
			<li>1500000</li>
			<li>2100000</li>
			<li>2400000</li>
			<li>2700000</li>
			<li>4500000</li>
		</ticksQuality>
		<results>
			<li>
				<thing>VCE_Cheese</thing>
				<count>25</count>
			</li>
		</results>
		<stopAtQuality>true</stopAtQuality>
		<allowExtractAtCurrentQuality>true</allowExtractAtCurrentQuality>
		<autoExtract>false</autoExtract>
	</PipeSystem.ProcessDef>
